FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0.07";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1.2;

boundaryField
{
    rotor
    {
        type            calculated;
        value           uniform 1.2;
    }
    stator
    {
        type            calculated;
        value           uniform 1.2;
    }
    AMI1
    {
        type            cyclicAMI;
        value           uniform 1.2;
    }
    AMI2
    {
        type            cyclicAMI;
        value           uniform 1.2;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


