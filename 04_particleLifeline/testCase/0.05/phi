FoamFile
{
    version     2.0;
    format      binary;
    class       surfaceScalarField;
    location    "0.05";
    object      phi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    rotor
    {
        type            calculated;
        value           uniform 0;
    }
    stator
    {
        type            calculated;
        value           uniform 0;
    }
    AMI1
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    AMI2
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    front
    {
        type            empty;
        value           nonuniform List<scalar> 
0
;
    }
    back
    {
        type            empty;
        value           nonuniform List<scalar> 
0
;
    }
}


